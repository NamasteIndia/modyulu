    <script>
        $('.btn-nav-menu, .bg-transparent, .site-overlay').click(function(){
            openMenu();
        });

        var isActive = false;

        function openMenu(){
            if (isActive){
                    $('.site-nav').removeClass('menu-open');
                    $('.site-overlay').removeClass('overlay-open');
            } else{
                $('.site-nav').addClass('menu-open');
                $('.site-overlay').addClass('overlay-open');
                
            }

            isActive = !isActive;
        }
        
    </script>
